{
  "name": "pasty-cli",
  "version": "0.0.4",
  "description": "Upload code or small files anonymously that are stored encrypted on S3.",
  "scripts": {
    "build": "tsc -p . --outDir dist/",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./main.ts",
    "server": "npm run build && node dist/main.js",
    "prepublishOnly": "tsc -p . --outDir dist/"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "mime": "^1.3.6",
    "pasty-core": "^0.1.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "@types/commander": "^2.9.1",
    "@types/mime": "^1.3.1",
    "@types/node": "^8.0.19",
    "@types/request": "^2.0.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.3"
  },
  "main": "dist/main.js",
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "bin": {
    "pasty": "dist/main.js"
  }
}
